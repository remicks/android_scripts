#!/bin/bash

if [ ! "$INIT" ]; then
	cd $(dirname $0)
	. ../headerfile
fi

if ( ! cat $LOCAL/config/outfile &> /dev/null ) \
|| [ $(cat $LOCAL/config/outfile | head -n 1) == "" ]; then
	outfile=$(echo $LOCAL/..)
else
	outfile=$(cat $LOCAL/config/outfile | head -n 1)
fi

tail -f $outfile/buildoutput-$(date +%Y%m%d).txt -n 100
